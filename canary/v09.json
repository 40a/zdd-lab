{
  "id": "/zdd/canary09",
  "instances": 9,
  "cpus": 0.1,
  "mem": 32,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "mhausenblas/simpleservice:0.4.0",
      "network": "HOST"
    }
  },
  "env": {
    "HEALTH_MIN": "1000",
    "HEALTH_MAX": "5000",
    "SIMPLE_SERVICE_VERSION": "0.9"
  },
  "healthChecks": [{
    "protocol": "HTTP",
    "path": "/health",
    "gracePeriodSeconds": 300,
    "intervalSeconds": 60,
    "timeoutSeconds": 20,
    "maxConsecutiveFailures": 3,
    "ignoreHttp1xx": false
  }],
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_PORT": "10099",
    "HAPROXY_0_VHOST": "http://ec2-52-25-126-14.us-west-2.compute.amazonaws.com",
    "HAPROXY_0_BACKEND_WEIGHT": "90"
  }
}
